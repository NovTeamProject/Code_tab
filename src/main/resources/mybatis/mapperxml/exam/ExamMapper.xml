    <!--    남원우님 여기 아래부터 작성 시작-->
<select id="answerTable" resultType="int"  parameterType="int">
    select count(*)
    from exam
    where exam_id = #{examId} and  exam_num = #{examNum}  and exam_answer = #{examAnswer}
</select>

